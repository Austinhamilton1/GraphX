;;; Simple Memory Test
;;; Read number from memory and add all numbers from 1 - that number

.mem
0x3 ; 3 in decimal

.code
MOVI Racc, 0
LD Rtmp1, 0 ; Read in 3 from memory
loop:
    ADD Racc, Racc, Rtmp1
    SUBI Rtmp1, Rtmp1, 1
    CMP Rtmp1, Rzero
    BNZ loop
ST Racc 1 ; Store the answer in memory
HALT